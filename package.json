{
  "name": "webpack-logging-plugin",
  "version": "0.0.0",
  "description": "Logging plugin for webpack",
  "main": "src/index.js",
  "files": [
    "*.md",
    "src"
  ],
  "scripts": {
    "prettify": "prettier --write \"{src,test}/**/*.js\"",
    "precommit": "lint-staged",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "<rootDir>/test/.*.test.js$",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "webpack",
    "plugin",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zinserjan/webpack-logging-plugin"
  },
  "bugs": {
    "url": "https://github.com/zinserjan/webpack-logging-plugin/issues"
  },
  "author": "Jan-Andr√© Zinser",
  "license": "MIT",
  "devDependencies": {
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "lint-staged": "^3.3.1",
    "prettier": "^0.22.0"
  }
}
