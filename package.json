{
  "name": "webpack-logging-plugin",
  "version": "0.1.1",
  "description": "Logging plugin for webpack",
  "main": "lib/WebpackLoggingPlugin.js",
  "files": [
    "*.md",
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && babel ./src -d lib",
    "prettify": "prettier --print-width 100 --write \"{src,test}/**/*.js\"",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "<rootDir>/test/.*.test.js$",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.js": [
      "prettier --print-width 100 --write",
      "git add"
    ]
  },
  "keywords": [
    "webpack",
    "plugin",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zinserjan/webpack-logging-plugin"
  },
  "bugs": {
    "url": "https://github.com/zinserjan/webpack-logging-plugin/issues"
  },
  "author": "Jan-AndrÃ© Zinser",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "lint-staged": "^3.3.1",
    "prettier": "^0.22.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "elegant-spinner": "^1.0.1",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "webpack": "^2.2.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
